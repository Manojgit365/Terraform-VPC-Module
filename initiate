### main.tf ###
terraform {
  required_providers {
    aws = {
      source = "hashicorp/aws"
    }
  }
}
provider "aws" {
  profile = "default"
  #region  = "us-east-1a"
}
module "vpc" {
  source                = "../vpc-module"
  vpc_name              = "my-vpc"
  cidr_block            = "10.0.0.0/16"
  enable_dns_hostnames  = true
  public_subnet_cidrs   = ["10.0.1.0/24"]
  private_subnet_cidrs  = ["10.0.2.0/24"]
  availability_zones    = ["us-east-1a"]
}

